{
  "compilerOptions": {
    "target": "es5",
    "types": [
      "core-js",
      "firebase",
      "jasmine",
      "node"
    ],
    "module": "commonjs",
    "noEmitHelpers": true,
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "es5"
    ],
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
